{
  "atomKeymap.promptV3Features": true,
  "editor.fontSize": 13,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.lineHeight": 22,
  "editor.renderIndentGuides": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
